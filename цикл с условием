def get_even_numbers_up_to(number): # объявление функции
    counter = 1 # начало отсчета 
    result = '' # нужно вернуть строку

    while counter <= number: # условие при отсчете от 1 до number (включительно)
        if counter % 2 == 0: # выборка четных чисел
          result += str(counter) + "," # строка должна сопровождаться запятыми
        elif counter==number: # доп. условие для последнего символа
          result += str(counter) + "." # последний символ тчк
        counter += 1 # увеличение на шаг
    return result # покажи результат цикла

print(get_even_numbers_up_to(9)) # функ. приняла аргумент 9
